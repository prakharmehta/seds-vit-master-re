<div class="whatwedo__card card">
  <div class="card-image waves-effect waves-block waves-light">
    <img class="activator" src="<%= divisions[i].img_src %>" />
  </div>
  <div class="card-content">
    <span class="card-title activator"
      ><%= divisions[i].name %><i class="material-icons right">more_vert</i></span
    >
    <!-- <p><a href="#">This is a link</a></p> -->
  </div>
  <div class="card-reveal">
    <span class="card-title"
      ><%= divisions[i].name %><i class="material-icons right">close</i></span
    >
    <p>
      <%= divisions[i].content %>
    </p>
  </div>
</div>
